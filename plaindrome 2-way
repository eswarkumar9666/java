class Main {
    public static void main(String[] args) {
        String s= "madam";
        boolean ans=true;
        int n = s.length();
        for(int i=0; i<n/2;i++){
            if(s.charAt(i) != s.charAt(n-i-1)){
                ans = false;
            }
        }
        if(ans){
            System.out.print("yes ! ede palindrome");
        }else{
            System.out.print("No! ede palindrome kadhu");
        }
    }
}
