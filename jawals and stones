class Main {
    public static void main(String[] args) {
        int ans = 0;
       String jewels = "aA"; 
       String stones = "aAAbbbb";
       for (int i=0;i<stones.length();i++){
           char ch = stones.charAt(i);
           for (int j=0;j<jewels.length();j++){
               char ch2 = jewels.charAt(j);
               if (ch == ch2){
                   ans += 1;
               }
           }
       }
       System.out.println(ans);
    }
}
