class Main {
    public static void main(String[] args) {
        String arr ="aba";
        String ans = "";
        for(int i=arr.length()-1; i>=0;i--){
            ans +=arr.charAt(i);
        }
        if(ans.equals(arr)){
            System.out.print("yes");
        }else{
            System.out.print("nop");
        }
    }
}
