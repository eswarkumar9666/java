class Main {
    public static void main(String[] args) {
       String address = "1.2.3.6.5.4";
       String ans = "";
       for (int i=0;i<address.length();i++){
           if (address.chatAt(i) == '.'){
               ans += "[.]";
           }else{
               ans += address.charAt(i);
           }
       }
       System.out.println(ans);
    }
}
